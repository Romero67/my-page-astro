---
const {page} = Astro.props

import '../../styles/global.scss'

switch (page) {
    case 'index':
        import '../../styles/pages/index.scss'
        import '../../scripts/slick/slick.css'
        import '../../scripts/slick/slick-theme.css'
        break;

    default:
        break;
}

